library(RSQLite)
# get current work directory
wd = getwd()
fpath = paste0(wd, "/")
dbfile = "MediaDB.db"
# create db or get db connection
dbcon <- dbConnect(RSQLite::SQLite(), paste0(fpath, dbfile))
dbDisconnect(dbcon)
